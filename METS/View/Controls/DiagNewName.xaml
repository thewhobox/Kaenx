<ContentDialog
    x:Class="METS.Classes.Controls.DiagNewName"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:METS.Classes.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Neuer Name"
    PrimaryButtonText="Umbenennen"
    CloseButtonText="Abbrechen"
    DefaultButton="Primary"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick">
    <ContentDialog.Resources>
        <CornerRadius x:Key="CornerRadius">0</CornerRadius>
        <CornerRadius x:Key="OverlayCornerRadius">0</CornerRadius>
        <CornerRadius x:Key="ControlCornerRadius">0</CornerRadius>
    </ContentDialog.Resources>
    
    <TextBox x:Name="Input" Text="Name" VerticalAlignment="Center" KeyUp="Input_KeyUp" />
</ContentDialog>
